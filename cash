#include <cs50.h>
#include <stdio.h>
#include <math.h>

int main(void)
{
float cash;
int j= 3;
do
{
    j= 3;
    cash= get_float("Change: ");
    if (cash>0)
    {
        j= 2;
    }

}
while (j==3);
//printf("%.2f\n",cash);
cash= cash * 100;
long money= cash;
//printf("%.2i\n",money);
long coins= 0;
while (money>=25)
{
    money= money-25;
    coins= coins+ 1;
}
while (money>=10)
{
    money= money-10;
    coins= coins+ 1;
}
while (money>=5)
{
    money= money-5;
    coins= coins+ 1;
}
while (money>=1)
{
    money= money-1;
    coins= coins+ 1;
}
printf("%.2li\n",coins);
}
