#include <cs50.h>
#include <stdio.h>
#include <math.h>

int main(void)
{

long isbn = get_long("ISBN?\n");

int digit10= isbn%10;
isbn= isbn/10;

int j=9;
int sum= 0;
int x;
for(int i=0;i<10;i++)
;
    {
    x= isbn%10;
    x= x*j;
    sum= x + sum;
    isbn=isbn/10;
    j--;
    }
sum= sum/11;
if (digit10 == sum)
    printf("TRUE ISBN\n");
}
